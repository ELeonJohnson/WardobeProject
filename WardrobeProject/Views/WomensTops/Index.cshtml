@model IEnumerable<WardrobeProject.Models.WomensTop>

@{
    ViewBag.Title = "Index";
}

<h2>Womens's Tops</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<div class="row all">
    @foreach (var item in Model)
    {


        <div class="col-6">
            <div>
                <img class="clothing-img" src="@Html.DisplayFor(modelItem => item.Photo)" alt="Alternate Text" />
            </div>

            <div class="icons">
                @Html.ActionLink("Edit", "Edit", new { id = item.WomensTopsID }) <img src="https://png.icons8.com/color/20/000000/multi-edit.png"> |
                @Html.ActionLink("Details", "Details", new { id = item.WomensTopsID }) <img src="https://png.icons8.com/ultraviolet/20/000000/search.png"> |
                @Html.ActionLink("Delete", "Delete", new { id = item.WomensTopsID }) <img src="https://png.icons8.com/ultraviolet/20/000000/cancel.png">
            </div>
        </div>


    }
</div>


